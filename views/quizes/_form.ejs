<div class="panel panel-default ">
  <div class="panel-heading">
    <h3 class="panel-title">Agregar Quiz</h3>
  </div>
  <div class="panel-body text-left">
	
	<label for="preg" class="rem">Pregunta:</label>
	
	<input type="text" class="form-control" id="preg" name="quiz[pregunta]" value="<%= quiz.pregunta%>" placeholder="" />
	
	<label for="resp" class="rem">Respuesta:</label>
	
	<input type="text" class="form-control" id="resp" name="quiz[respuesta]" value="<%= quiz.respuesta%>" placeholder=""/>
	<div style="margin: 10px 0px" class="text-right">
	<input type="submit" value="Guardar" class="btn btn-primary">
	<a href="/quizes" class="btn btn-warning">Cancelar</a>
	</div>

	

  </div>
</div>