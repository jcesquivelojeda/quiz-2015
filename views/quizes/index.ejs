<form method="get" action="/quizes" enctype="application/x-www-form-urlencoded">
<label for="search">Buscar</label>
<input type="text" name="search" id="search" autocomplete="off" placeholder="ingrese palabras clave"/>
<input type="submit" value="buscar">
</form>
<table>
<% 
var i=0; 
for(i=0;i<quizes.length;i++){ %>
<tr>
	<td>
		<a href="quizes/<%= quizes[i].id %>"> <%= quizes[i].pregunta %>
		</a>
	</td>
</tr>
<% } %>
</table>
